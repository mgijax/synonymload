#!/bin/csh -f

#
# Wrapper script to create & load new synonyms
#
# Usage:  synonymload.csh
#

setenv SCHEMADIR	/usr/local/mgi/live/dbutils/mgd/mgddbschema
setenv INPUTFILE	?
setenv MODE		preview
setenv OBJECTTYPE	Strain
setenv SYNLOAD		/usr/local/mgi/live/dataload/synonymload/synonymload.py

source ${SCHEMADIR}/Configuration
setenv LOG	$0.log

date >& $LOG

${SYNLOAD} -S${DBSERVER} -D${DBNAME} -U${DBUSER} -P${DBPASSWORDFILE} -M${MODE} -O${OBJECTTYPE} -I${INPUTFILE} >>& $LOG

date >>& $LOG

